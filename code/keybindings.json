[
  {
    "key": "space f",
    "command": "workbench.action.quickOpen",
    "when": "vim.mode != 'Insert' && textInputFocus"
  },
  {
    "key": "space c",
    "command": "workbench.action.showCommands",
    "when": "vim.mode != 'Insert' && textInputFocus"
  },
  {
    "key": "shift+cmd+p",
    "command": "-workbench.action.showCommands"
  }
]
